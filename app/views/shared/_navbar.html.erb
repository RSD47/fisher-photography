<div class="navbar navbar-expand-sm bg-black navbar-lewagon">
  <div>
    <%= link_to collections_path, class: "home-link" do %>
      <img src="<%= asset_path('fisher_logo.png') %>" class="nav-img">
      <h3 class="m-2 ps-2 text-light nav-title">Fisher Photography</h3>
    <% end %>
  </div>
  <% if user_signed_in? %>
    <%= link_to "Log out", destroy_user_session_path, data: {turbo_method: :delete}, class: "signup button" %>
  <% else %>
    <%= link_to "Sign up", new_user_registration_path, class: "signup button" %>
  <% end %>
</div>
